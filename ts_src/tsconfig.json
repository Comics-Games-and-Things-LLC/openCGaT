{
  "compilerOptions": {
    "jsx": "react",
    "module": "ES2020",
    "moduleResolution": "Node",
    "noImplicitAny": true,
    "outDir": "../static/js/",
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "lib": [
      "dom",
      "webworker",
      "es2017"
    ],
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "react_index.tsx",
    "components/ui_constants.ts",
    "index.d.ts",
  ]
}